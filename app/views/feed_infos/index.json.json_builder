array @feed_infos do |fi|
	_id fi.id
	title fi.title.gsub(/\"/,'\"').gsub(/\,\,/,",").gsub(/\r/,"")
	address fi.address.gsub(/\"/,'\"').gsub(/\,\,/,",").gsub(/\r/,"")
	category fi.category
	follower fi.follower.to_i
	profiles Profile.where({:_id.in => fi.feed_info_profiles.map(&:profile_id)}).to_a do |profile|
		_id profile.id
		title profile.title
		is_private profile.is_private
		profile_category profile.profile_category do |pc|
			_id pc.id
			title pc.title
		end
	end
end